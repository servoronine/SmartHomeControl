<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
    >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="70.00dp"
        android:text="Source"
        android:id="@+id/textView9"
        android:layout_marginTop="59.33dp"
        android:background="@drawable/border"
        android:textColor="#f8f8f8"
        android:textSize="20.67dp"
        android:textIsSelectable="true"
        android:linksClickable="false"
        android:singleLine="false"
        android:textAlignment="gravity"
        android:gravity="fill_vertical|center_horizontal"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="77.33dp"
        android:layout_below="@+id/textView9"
        android:layout_centerHorizontal="true"
        android:id="@+id/linearLayout"
        android:background="@drawable/button_background"
        android:paddingRight="25.33dp"
        android:clickable="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight=".50">

            <FrameLayout
                android:layout_width="78.00dp"
                android:layout_height="match_parent">

                <ImageView
                    android:layout_width="28.67dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView6"
                    android:src="@drawable/tv_tv"
                    android:layout_gravity="center" />
            </FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="TV"
                android:id="@+id/textView10"
                android:textColor="#f8f8f8"
                android:textAlignment="gravity"
                android:gravity="center|clip_vertical"
                android:textSize="22.00dp" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="New Text"
            android:id="@+id/textView11"
            android:layout_weight=".50"
            android:textColor="#f8f8f8"
            android:textSize="22.00dp"
            android:textIsSelectable="true"
            android:gravity="center_vertical|right" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="77.33dp"
        android:id="@+id/linearLayout2"
        android:paddingRight="25.33dp"
        android:layout_below="@+id/linearLayout"
        android:background="@drawable/button_background"
        android:layout_centerHorizontal="true"
        android:clickable="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight=".50" >

            <FrameLayout
                android:layout_width="78.00dp"
                android:layout_height="match_parent" >

                <ImageView
                    android:layout_width="34.00dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView7"
                    android:src="@drawable/tv_network"
                    android:layout_gravity="center" />
            </FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Network"
                android:id="@+id/textView12"
                android:textColor="#f8f8f8"
                android:textAlignment="gravity"
                android:gravity="center|clip_vertical"
                android:textSize="22.00dp" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="----"
            android:id="@+id/textView13"
            android:layout_weight=".50"
            android:textColor="#f8f8f8"
            android:textSize="22.00dp"
            android:textIsSelectable="true"
            android:gravity="center_vertical|right" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="77.33dp"
        android:id="@+id/linearLayout3"
        android:paddingRight="25.33dp"
        android:layout_below="@+id/linearLayout2"
        android:background="@drawable/button_background"
        android:layout_centerHorizontal="true"
        android:clickable="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight=".50" >

            <FrameLayout
                android:layout_width="78.00dp"
                android:layout_height="match_parent" >

                <ImageView
                    android:layout_width="22.00dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView8"
                    android:src="@drawable/tv_usb"
                    android:layout_gravity="center" />
            </FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="USB "
                android:id="@+id/textView14"
                android:textColor="#f8f8f8"
                android:textAlignment="gravity"
                android:gravity="center|clip_vertical"
                android:textSize="22.00dp" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Flash 8 gb"
            android:id="@+id/textView15"
            android:layout_weight=".50"
            android:textColor="#f8f8f8"
            android:textSize="22.00dp"
            android:textIsSelectable="true"
            android:gravity="center_vertical|right" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="77.33dp"
        android:id="@+id/linearLayout4"
        android:paddingRight="25.33dp"
        android:layout_below="@+id/linearLayout3"
        android:background="@drawable/button_background"
        android:layout_centerHorizontal="true"
        android:clickable="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight=".50" >

            <FrameLayout
                android:layout_width="78.00dp"
                android:layout_height="match_parent" >

                <ImageView
                    android:layout_width="51.33dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView9"
                    android:src="@drawable/tv_hdmi"
                    android:layout_gravity="center" />
            </FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="HDMI"
                android:id="@+id/textView16"
                android:textColor="#f8f8f8"
                android:textAlignment="gravity"
                android:gravity="center|clip_vertical"
                android:textSize="22.00dp" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="----"
            android:id="@+id/textView17"
            android:layout_weight=".50"
            android:textColor="#f8f8f8"
            android:textSize="22.00dp"
            android:textIsSelectable="true"
            android:gravity="center_vertical|right" />
    </LinearLayout>
</RelativeLayout>