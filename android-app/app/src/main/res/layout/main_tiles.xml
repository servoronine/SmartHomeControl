                    android:layout_marginTop="38dp"
                    android:layout_marginBottom="7dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Garden"
                    android:id="@+id/textView31"
                    android:layout_gravity="bottom|center_horizontal"
                    android:textColor="#f8f8f8"
                    android:textSize="14sp"
                    android:layout_below="@+id/imageView50"
                    android:layout_centerHorizontal="true"/>
            </RelativeLayout>

            <RelativeLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_background"
                android:paddingTop="5dp"
                android:paddingBottom="7dp"
                android:clickable="true"
                android:focusable="true"
                android:layout_weight="1">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:id="@+id/imageView51"
                    android:src="@drawable/main_hallway"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="38dp"
                    android:layout_marginBottom="7dp"
                    android:layout_alignParentTop="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Hallway"
                    android:id="@+id/textView32"
                    android:layout_gravity="center_horizontal"
                    android:textColor="#f8f8f8"
                    android:textSize="14sp"
                    android:layout_below="@+id/imageView51"
                    android:layout_centerHorizontal="true"/>
            </RelativeLayout>

            <RelativeLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_background"
                android:paddingTop="5dp"
                android:paddingBottom="7dp"
                android:clickable="true"
                android:focusable="true"
                android:layout_weight="1">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:id="@+id/imageView52"
                    android:src="@drawable/main_front_door"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="38dp"
                    android:layout_marginBottom="7dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Front door"
                    android:id="@+id/textView33"
                    android:layout_gravity="center_horizontal"
                    android:textColor="#f8f8f8"
                    android:textSize="14sp"
                    android:layout_below="@+id/imageView52"
